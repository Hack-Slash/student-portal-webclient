<h1><%= @student['first_name'] %> <%= @student['last_name'] %></h1>

<h3>General Info</h3>
<hr>
<p><strong>email: </strong><%= @student['email'] %></p>
<p><strong>phone_number: </strong><%= @student['phone_number'] %></p>
<p><strong>short_bio: </strong><%= @student['short_bio'] %></p>
<p><strong>linkedin_url: </strong><%= @student['linkedin_url'] %></p>
<p><strong>twitter_handle: </strong><%= @student['twitter_handle'] %></p>
<p><strong>personal_web_url: </strong><%= @student['personal_web_url'] %></p>
<p><strong>online_resume_url: </strong><%= @student['online_resume_url'] %></p>
<p><strong>github_url: </strong><%= @student['github_url'] %></p>
<p><strong>photo: </strong><%= @student['photo'] %></p>
<hr>
<hr>
<hr>

<h3>Experience</h3>
<% @experiences.each do |experience| %>
	<hr>
	<p><strong>start_date: </strong><%= experience['start_date'] %></p>
	<p><strong>end_date: </strong><%= experience['end_date'] %></p>
	<p><strong>job_title: </strong><%= experience['job_title'] %></p>
	<p><strong>company_name: </strong><%= experience['company_name'] %></p>
	<p><strong>details: </strong><%= experience['details'] %></p>
<% end %>
<hr>
<hr>
<hr>

<h3>Education</h3>
<% @education.each do |education| %>
	<hr>
	<p><strong>start_date: </strong><%= education['start_date'] %></p>
	<p><strong>end_date: </strong><%= education['end_date'] %></p>
	<p><strong>degree: </strong><%= education['degree'] %></p>
	<p><strong>university: </strong><%= education['university'] %></p>
	<p><strong>details: </strong><%= education['details'] %></p>
<% end %>
<hr>
<hr>
<hr>

<h3>Capstones</h3>
<% @capstones.each do |capstone| %>
	<hr>
	<p><strong>name: </strong><%= capstone['name'] %></p>
	<p><strong>description: </strong><%= capstone['description'] %></p>
	<p><strong>url: </strong><%= capstone['url'] %></p>
	<p><strong>screenshot: </strong><%= capstone['screenshot'] %></p>
<% end %>
<hr>
<hr>
<hr>

<h3>Skills</h3>
<% @skills.each do |skill| %>
	<hr>
	<p><strong>skill_name: </strong><%= skill['skill_name'] %></p>
<% end %>